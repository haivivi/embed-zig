# Std library - Standard library trait implementations (std_impl)

load("//bazel/zig:defs.bzl", "zig_lib")

package(default_visibility = ["//visibility:public"])

zig_lib(
    name = "std",
    lib_name = "std_impl",  # Zig module name differs from target name
    srcs = glob(["**/*"]),
    deps = ["//lib/trait"],
)
