# Std library - Standard library extensions (std_sal)

load("//bazel/zig:defs.bzl", "zig_lib")

package(default_visibility = ["//visibility:public"])

zig_lib(
    name = "std",
    lib_name = "std_sal",  # Zig module name differs from target name
    srcs = glob(["**/*"]),
    deps = ["//lib/trait"],
)
